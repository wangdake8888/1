<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
         <class name="edu.tsinghua.bean.Article" table="article">
              <id name="articleId" type="int">
                  <generator class="identity"></generator>
              </id>
              <property name="articleBiaoTi" type="java.lang.String"></property>
              <property name="articleZhaiYao" type="java.lang.String"></property>
              <property name="articleNeiRong" type="java.lang.String"></property>
              <property name="ShiJian" type="java.lang.String"></property>
         </class>
    
    </hibernate-mapping>