<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
	    <class name="edu.tsinghua.bean.User" table="user">
		    <id name="userId" type="int">
		     	<generator class="identity"></generator>
		    </id>
	    	 <property name="userName" type="java.lang.String"></property>
	    	 <property name="passWord" type="java.lang.String"></property>
	    	  <property name="userType" type="java.lang.String"></property>
	    </class>
    
    
    </hibernate-mapping>